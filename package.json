{
  "name": "styled-jsx",
  "version": "2.2.6",
  "license": "MIT",
  "repository": "zeit/styled-jsx",
  "description": "Full CSS support for JSX without compromises",
  "files": [
    "dist",
    "lib",
    "server.js",
    "babel.js",
    "style.js",
    "css.js"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-stage-3",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "scripts": {
    "dev": "gulp",
    "prepublish": "gulp transpile",
    "test": "npm run lint && ava",
    "lint": "xo",
    "format": "prettier --single-quote --no-semi --write *.js {src,test}/*.js",
    "precommit": "xo --quiet && lint-staged"
  },
  "ava": {
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions" : {
        "plugins": ["@babel/plugin-syntax-jsx"],
        "presets": ["@babel/preset-stage-3"]
      }
    }
  },
  "xo": {
    "envs": [
      "node",
      "browser"
    ],
    "extends": "prettier",
    "ignores": [
      "lib/style-transform.js"
    ],
    "rules": {
      "capitalized-comments": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "prettier --single-quote --no-semi --write",
      "npm test",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "7.0.0-beta.49",
    "@babel/plugin-syntax-jsx": "7.0.0-beta.49",
    "@babel/runtime": "7.0.0-beta.49",
    "@babel/types": "7.0.0-beta.49",
    "convert-source-map": "1.5.1",
    "source-map": "0.7.3",
    "string-hash": "1.1.3",
    "stylis": "3.5.0",
    "stylis-rule-sheet": "0.0.10"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.49",
    "@babel/core": "7.0.0-beta.44",
    "@babel/plugin-transform-runtime": "7.0.0-beta.49",
    "@babel/preset-env": "7.0.0-beta.49",
    "@babel/preset-react": "7.0.0-beta.49",
    "@babel/preset-stage-3": "7.0.0-beta.49",
    "@babel/register": "7.0.0-beta.49",
    "ava": "1.0.0-beta.5.1",
    "babel-preset-minify": "^0.4.3",
    "benchmark": "^2.1.4",
    "eslint-config-prettier": "^2.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-benchmark": "^1.1.1",
    "human-size": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "mz": "^2.7.0",
    "prettier": "^1.13.5",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-jsdom": "^2.0.5",
    "xo": "^0.21.1"
  },
  "peerDependencies": {
    "react": "15.x.x || 16.x.x"
  },
  "keywords": [
    "babel-plugin",
    "zeit",
    "css-in-js",
    "css"
  ]
}
